<%for(var i =0; i < items.length; i++){%>
	<%var item= items[i]%>
	<tr <%= item%> class='<%= i==0 ? "first-row" : ""%>'>
		<%$.each(options.columns, function(name, title){%>
			<td><%= item[name] %></td>
		<%})%>
	</tr>

<%}%>
<tr class='spacing'>
	<%$.each(options.columns, function(name, title){%>
		<td><div class='spacer'><%= title %></div></td>
	<%})%>
</tr>